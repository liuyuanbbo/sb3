<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kafka3.mapper.dataopen.ObjInfoMapper">
    <resultMap id="baseResultMap" type="kafka3.model.ObjInfo">
        <id column="object_id" property="objectId" jdbcType="BIGINT"/>
        <result column="parent_id" property="parentId" jdbcType="BIGINT"/>
        <result column="grp_id" property="grpId" jdbcType="BIGINT"/>
        <result column="object_code" property="objectCode" jdbcType="VARCHAR"/>
        <result column="object_name" property="objectName" jdbcType="VARCHAR"/>
        <result column="primary_key" property="primaryKey" jdbcType="VARCHAR"/>
        <result column="meta_table_id" property="metaTableId" jdbcType="BIGINT"/>
        <result column="table_tode" property="tableCode" jdbcType="VARCHAR"/>
        <result column="table_type" property="tableType" jdbcType="VARCHAR"/>
        <result column="status_cd" property="statusCd" jdbcType="VARCHAR"/>
        <result column="layer" property="layer" jdbcType="INTEGER"/>
        <result column="domain" property="domain" jdbcType="INTEGER"/>
        <result column="comments" property="comments" jdbcType="INTEGER"/>
        <result column="create_type" property="createType" jdbcType="VARCHAR"/>
        <result column="create_date" property="createDate" jdbcType="DATE"/>
        <result column="oper_id" property="operId" jdbcType="BIGINT"/>
        <result column="com_acct_id" property="comAcctId" jdbcType="BIGINT"/>
        <result column="dir_id" property="dirId" jdbcType="BIGINT"/>
        <result column="datasource_id" property="datasourceId" jdbcType="BIGINT"/>
        <result column="data_cnt_type" property="dataCntType" jdbcType="VARCHAR"/>
        <result column="object_alias" property="objectAlias" jdbcType="VARCHAR"/>
        <result column="project_id" property="projectId" jdbcType="BIGINT"/>
        <result column="key_code" property="keyCode" jdbcType="VARCHAR"/>
        <result column="order_no" property="orderNo" jdbcType="BIGINT"/>
    </resultMap>

    <sql id="baseSql">
        job_id,job_title,min_salary,max_salary
    </sql>

    <select id="getJobs" resultMap="baseResultMap">
        select
        <include refid="baseSql"/>
        from jobs
    </select>

    <select id="selectNow" resultType="java.time.LocalDate">
        select now()
    </select>
</mapper>