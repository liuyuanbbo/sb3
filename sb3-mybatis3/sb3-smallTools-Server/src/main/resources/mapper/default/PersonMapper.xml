<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zjc.smalltools.mapper.PersonMapper">
    <insert id="insert">
        insert into tb_person(name,age,email) values (#{person.name},#{person.age},#{person.email})
    </insert>
</mapper>