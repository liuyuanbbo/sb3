<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="zzjjcc.mapper.mysql.hrdb.CommonCodeToNameMapper">
    <select id="commonCodeToName" resultType="java.util.Map">
        select ${itemName}
        from ${tableName}
        where ${dbCodeField}=#{codeValue}
    </select>
</mapper>